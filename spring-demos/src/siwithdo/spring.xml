<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="address" class="siwithdo.Address">
		<property name="addressLine" value="4-10"></property>
		<property name="city" value="KDC-Gpur"></property>
		<property name="state" value="Warangal"></property>
		<property name="country" value="India"></property>
	</bean>

	<bean id="emp" class="siwithdo.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Seetaram"></property>
		<property name="address" ref="address"></property>
	</bean>

</beans>  